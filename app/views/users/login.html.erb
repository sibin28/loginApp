
<div>
<h1>Login</h1>
<%= form_for(:user, :url => {:controller => 'users', :action => 'login'}) do |f| %>
<p> Username:</br> <%= f.text_field :username%> </p>
<p> Password:</br> <%= f.password_field :password%></p>

<%= f.submit :Login %>
<% end %>

<%=link_to "Add New User", :controller => "users", :action => "new"%>

</div>


